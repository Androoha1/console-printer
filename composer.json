{
    "name": "posternak/console-printer",
    "description": "A tool to print messages in the terminal.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrii Posternak"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Posternak\\ConsolePrinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
